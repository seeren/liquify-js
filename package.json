{
  "name": "liquify-js",
  "description": "Liquify filter for HTML elements",
  "version": "1.0.4",
  "keywords": [
    "liquify",
    "web-gl",
    "warp",
    "filter"
  ],
  "homepage": "https://seeren.github.io/liquify-js/",
  "author": "Cyril Ichti",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --mode development --watch",
    "prod": "webpack --mode production --watch",
    "build": "webpack --mode production",
    "test": "mocha --require @babel/register --recursive test/unit",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --recursive test/unit",
    "test:coveralls": "npm run test:coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seeren/liquify-js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/register": "latest",
    "babel-loader": "latest",
    "babel-plugin-istanbul": "latest",
    "browser-sync": "latest",
    "browser-sync-webpack-plugin": "latest",
    "chai": "latest",
    "coveralls": "latest",
    "cross-env": "latest",
    "html2canvas": "latest",
    "jsdom": "latest",
    "mocha": "latest",
    "mocha-lcov-reporter": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "style-loader": "latest",
    "three": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "engines": {
    "node": ">=8"
  }
}
